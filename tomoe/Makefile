run:
	@echo '=========== 50%, 50%, 50%'
	./tomoe.rb 1000 | ../count.rb
	@echo '=========== 50%, 45%, 55%'
	./tomoe.rb -p=50,45,55 1000 | ../count.rb
	@echo '=========== 50%, 40%, 60%'
	./tomoe.rb -p=50,40,60 1000 | ../count.rb
	@echo '=========== 50%, 35%, 65%'
	./tomoe.rb -p=50,35,65 1000 | ../count.rb
	@echo '=========== 50%, 30%, 70%'
	./tomoe.rb -p=50,30,70 1000 | ../count.rb

long:
	@echo '=========== 50%, 50%, 50%'
	./tomoe.rb 100000 | ../count.rb
	@echo '=========== 50%, 45%, 55%'
	./tomoe.rb -p=50,45,55 100000 | ../count.rb
	@echo '=========== 50%, 40%, 60%'
	./tomoe.rb -p=50,40,60 100000 | ../count.rb
	@echo '=========== 50%, 35%, 65%'
	./tomoe.rb -p=50,35,65 100000 | ../count.rb
	@echo '=========== 50%, 30%, 70%'
	./tomoe.rb -p=50,30,70 100000 | ../count.rb
